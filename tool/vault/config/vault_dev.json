{
  "backend": {
    "file": {
      "path": "/vault/file"
    }
  },
  "listener": {
    "tcp":{
      "address": "0.0.0.0:8200",
      "tls_disable": 1
    }
  },
  "storage": [{
    "file": {
      "path": "/vault/data"
    }
  }],
  "log_level": "info",
  "log_file": "/vault/logs/vault.log",
  "ui": true,
  "dev": true
}
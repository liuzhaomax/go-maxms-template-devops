{
  "ui": true,
  "storage": {
    "file": {
      "path": "/vault/data"
    }
  },
  "backend": {
    "file": {
      "path": "/vault/file"
    }
  },
  "listener": {
    "tcp":{
      "address": "0.0.0.0:8200",
      "cluster_addr": "0.0.0.0:8201",
      "tls_disable": true
    }
  },
  "log_level": "info",
  "log_file": "/vault/logs/vault.log",
  "plugin_directory": "/vault/plugins"
}
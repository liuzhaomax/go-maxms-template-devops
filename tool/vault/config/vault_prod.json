{
  "listener": [{
    "tcp": {
      "address": "0.0.0.0:8200",
      "tls_disable": 0,
      "tls_cert_file": "/path/to/tls_cert.pem",
      "tls_key_file": "/path/to/tls_key.pem"
    }
  }],
  "storage": [{
    "file": {
      "path": "/vault/data"
    }
  }],
  "log_level": "info",
  "log_file": "/vault/logs/vault.log",
  "api_addr": "https://0.0.0.0:8200",
  "cluster_addr": "https://0.0.0.0:8201",
  "ui": true
}